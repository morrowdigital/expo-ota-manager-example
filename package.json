    "react": "18.2.0",
    "react-native": "0.72.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.14",
    "env-cmd": "^10.1.0",
    "typescript": "^5.1.3"
  },
  "private": true
}
